@article{elamin_etal23,
  title = {Open {{Science Practices}} in {{Communication Sciences}} and {{Disorders}}: {{A Survey}}},
  shorttitle = {Open {{Science Practices}} in {{Communication Sciences}} and {{Disorders}}},
  author = {El Amin, Mariam and Borders, James C. and Long, Helen L. and Keller, Mary Alice and Kearney, Elaine},
  date = {2023},
  journaltitle = {Journal of Speech, Language, and Hearing Research},
  volume = {66},
